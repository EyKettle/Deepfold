{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "deepfold",
  "version": "0.1.7",
  "identifier": "com.eykettle.deepfold",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Deepfold",
        "width": 400,
        "height": 740,
        "minWidth": 400,
        "minHeight": 530,
        "decorations": false,
        "transparent": true,
        "windowEffects": {
          "effects": ["tabbed", "mica", "acrylic"]
        },
        "visible": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": false,
    "targets": "all",
    "publisher": "EyKettle",
    "copyright": "Belong to EyKettle",
    "shortDescription": "A AI assistant app",
    "longDescription": "A AI assistant app",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
